
import java.util.Arrays;

public class exercitiuZece {

	public static void main(String[] args) {

		int maxim[] = { 11, 55, 84, 67, 99, 245, 2 };

		Arrays.sort(maxim);

		System.out.print(Arrays.toString(maxim));
		System.out.println(" ");

		System.out.println(maxim[maxim.length -8]);
		System.out.println(maxim[maxim.length -1]);
		System.out.println(maxim[maxim.length -8]);


	}

}
