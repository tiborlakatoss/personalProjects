
public class sum {

	public static void main(String[] args) {

		int suma[] = { 22, 98, 65, -33, 87, 2 };
		for (int i = 0; i < suma.length; i++) {
			System.out.println(suma[i] + "");
		}

		int total = 0;

		for (int i = 0; i < suma.length; i++)
			;
		{
			total += suma[i];

		}

		System.out.println("total is " + total);
	}
}
