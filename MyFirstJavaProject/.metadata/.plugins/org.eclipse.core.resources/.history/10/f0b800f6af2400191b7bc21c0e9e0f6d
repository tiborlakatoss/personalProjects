package SchoolPractice;

public class Staff extends Person {
	private String school;
	private Double pay;

	public Staff() {
	}

	public Staff(String school, Double pay) {

		this.school = school;
		this.pay = pay;
	}


	@Override
	public String toString() {
		return "Staff [school=" + school + ", pay=" + pay + "]";
	}

}
