package com.sda.tibi.fundamentals.moreHomework;

import java.util.Scanner;

public class nAddingSum {

	public static void main(String[] args) {
//		44. Write a Java program that accepts an integer (n) and computes the value of n+nn+nnn. Go to the editor
//		Sample Output:
//
//		Input number: 5                                                        
//		5 + 55  + 555
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();

		int[] firstn = new int[n];
		int[][] secondn = new int[n][n];
		int[][][] thirdn = new int[n][n][n];
		System.out.println(firstn + secondn + thirdn);

	}

}
