package SchoolPractice;

public class Student extends Person {

	private String program;
	private int year;
	private double fee;

	public Student() {
	}

	public Student(String program, int year, double fee) {

		this.program = program;
		this.year = year;
		this.fee = fee;

	}

	@Override
	public String toString() {
		return "Student [program=" + program + ", year=" + year + ", fee=" + fee + "]";
	}

}
